{
  "name": "my-webpack-project",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "axios": "^1.2.0",
    "axios-hooks": "^4.0.0",
    "classnames": "^2.3.2",
    "crypto-hash": "^2.0.1",
    "history": "^5.3.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.5",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.6",
    "redux": "^4.2.0",
    "utility-types": "^3.10.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env node_modules/.bin/react-scripts start",
    "server-start": "cross-env node_modules/.bin/json-server --watch ./src/data/db.json --port 3030 --delay 1000 -m ./node_modules/json-server-auth",
    "start-with-server": "cross-env node_modules/.bin/concurrently \\\"json-server --watch ./src/data/db.json --port 3030 --delay 1000\\\" \\\"react-scripts start\\\"",
    "build": "cross-env node_modules/.bin/webpack --mode=production --define-process-env-node-env=production",
    "test": "cross-env node_modules/.bin/react-scripts test",
    "eject": "cross-env node_modules/.bin/react-scripts eject",
    "build:dev": "cross-env node_modules/.bin/webpack --mode=development",
    "build:prod": "cross-env node_modules/.bin/webpack --mode=production --define-process-env-node-env=production",
    "watch": "cross-env node_modules/.bin/webpack --watch",
    "serve": "cross-env node_modules/.bin/webpack serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@webpack-cli/generators": "^3.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "json-server": "^0.17.1",
    "json-server-auth": "^2.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "description": "My webpack project"
}
